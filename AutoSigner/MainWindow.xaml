<Window x:Class="AutoSigner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoSigner"
        xmlns:vw="clr-namespace:AutoSigner.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="299.369" Width="527.65" ResizeMode="NoResize">
    <Window.DataContext>
        <Binding Source="{StaticResource client}" />
    </Window.DataContext>
    <Window.TaskbarItemInfo>
        <TaskbarItemInfo ProgressValue="{Binding Progress}" ProgressState="Normal" />
    </Window.TaskbarItemInfo>
    <vw:SignInClientView />
</Window>
